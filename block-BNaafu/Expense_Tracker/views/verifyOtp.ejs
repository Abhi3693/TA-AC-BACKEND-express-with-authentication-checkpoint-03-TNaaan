<%- include('partials/header') %>
<main>
  <section class="forgot-sec">
    <div class="container">
      <div>
        <div>
          <form class="form-control" action="/verifyEmail/<%= id %>/verifyOtp" method="POST">
            <div class="form-img">
              <img src="data:image/png;base64," alt="">
            </div>
            <h3>
              Check your email we have sent OTP for email verification 
            </h3>
            <% if (error) { %>
            <p class="err-msg"><%= error %></p>
            <% } %>
            <input type="email" name="email" value="<%= content.email %>" />
            <input
              type="password"
              required
              id="email"
              name="otp"
              placeholder="Enter OTP"
            />
            <button class="btn" type="submit">Verify OTP</button>
          </form>
        </div>
      </div>
    </div>
  </section>
</main>
<%- include('partials/footer') %>

