<% include partials/header %>
<main>
  <section>
    <div class="container ">
      
      <div class="">
        <div>
          <form class="form-control" action="/verifyEmail/<%= data._id %>/verifyOtp/forgot" method="POST">
            <div class="form-img">
              <img
                src="data:image/jpeg;base64,"
                alt=""
                class="img"
              />
            </div>
            <h3>
              Check your email we have sent OTP for email verification 
            </h3>
            <% if (error) { %>
            <p><%= error %></p>
            <% } %>
            <input
              type="email"
              name="email"
              value="<%= data.email %>"
              class="form-control diseable"
            />
            <input
              type="string"
              required
              id="email"
              name="otp"
              class="form-control"
              placeholder="Enter your OTP"
            />
            <input type="submit" class="btn form-control" value="Verify OTP"></input>
          </form>
        </div>
      </div>
    </div>
  </section>
</main>
<% include partials/footer %>